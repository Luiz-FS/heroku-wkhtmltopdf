#!/bin/bash

DIST_VERSION=$(lsb_release -c | awk '{print $2}');

wget https://downloads.wkhtmltopdf.org/0.12/0.12.5/wkhtmltox_0.12.5-1.$(echo $DIST_VERSION)_amd64.deb

dpkg -i wkhtmltox_0.12.5-1.$(echo $DIST_VERSION)_amd64.deb
apt-get -f install
rm wkhtmltox_0.12.5-1.$(echo $DIST_VERSION)_amd64.deb
